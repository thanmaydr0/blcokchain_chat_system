# ============================================
# DuoGraph Frontend - Environment Variables
# ============================================
# Copy this file to .env and fill in the values
# NEVER commit the .env file to version control!

# ============================================
# SUPABASE (Required)
# ============================================

# Your Supabase project URL
# Get from: https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co

# Supabase anonymous/public key
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# ============================================
# BLOCKCHAIN - Ethereum Sepolia
# ============================================

# Sepolia RPC URL
VITE_SEPOLIA_RPC=https://rpc.sepolia.org

# Chain ID for Sepolia
VITE_CHAIN_ID=11155111

# ============================================
# CONTRACT ADDRESSES (Deployed to Sepolia)
# ============================================

VITE_PACT_FACTORY_ADDRESS=0x47cE94bB1bedd7953Fb3917f37A28A79521cbFEB
VITE_ACCOUNT_FACTORY_ADDRESS=0xb355C259cbAF5c49e2768F166b3b0aCA37188c70
VITE_PAYMASTER_ADDRESS=0x2C8436727b1a1fC67A1b4d028cec6dffCCe1Ecb0

# ============================================
# IPFS (Optional - for media sharing)
# ============================================

# Pinata API credentials for IPFS uploads
# Get from: https://app.pinata.cloud/keys
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_SECRET=your_pinata_secret_key

# IPFS Gateway URL
VITE_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/

# IPFS API Endpoint
VITE_IPFS_API=https://api.pinata.cloud

# ============================================
# ERC-4337 (Account Abstraction)
# ============================================

# EntryPoint contract address
VITE_ENTRYPOINT_ADDRESS=0x5FF137D4b0FDCD49DcA30c7CF57E578a026d2789

# Bundler RPC for gasless transactions (Pimlico, Stackup, etc.)
# Get API key from: https://www.pimlico.io/
VITE_BUNDLER_RPC=https://api.pimlico.io/v2/sepolia/rpc?apikey=YOUR_API_KEY

